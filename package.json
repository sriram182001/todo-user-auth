{
  "name": "node-react-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "server/server.js",
  "scripts": {
    "start": "npm-run-all --parallel client server",
    "server": "nodemon",
    "start-debug": "npm-run-all --parallel client debug",
    "debug": "nodemon --inspect",
    "bootstrap": "node server/bootstrap.js",
    "client": "webpack-dev-server --mode development",
    "build": "webpack --mode production",
    "migrate": "node server/bootstrap.js",
    "migrate:up": "node server/migrate.js up",
    "repl": "node server/repl",
    "clean": "node build-scripts/clean.js",
    "copy:images": "copyfiles -f client/app/images/* client/dist/images",
    "copy:fonts": "copyfiles -f client/app/fonts/* client/dist/fonts",
    "copy:assets": "copyfiles -f client/app/assets/* client/dist/assets",
    "publish": "node build-scripts/publish.js",
    "prod-build:webpack": "node --max_old_space_size=4096 node_modules/.bin/webpack --config webpack.production.config.js",
    "prod-build:sass": "node build-scripts/build-sass.js",
    "prod-build:js": "npm-run-all --parallel prod-build:webpack*",
    "prod-build": "npm-run-all -p prod-build:js prod-build:sass -s copy:*",
    "prod": "npm-run-all prod-build publish",
    "intialize": "node ./.setup/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@blueprintjs/datetime": "^3.13.0",
    "@sentry/webpack-plugin": "^1.8.1",
    "app-module-path": "^2.2.0",
    "bell": "~8.5.0",
    "catbox-redis": "^3.1.1",
    "config": "^3.3.7",
    "handlebars": "^4.7.7",
    "hapi": "16.1.0",
    "hapi-auth-basic": "~4.2.0",
    "hapi-auth-cookie": "~7.0.0",
    "hapi-auth-jwt2": "~7.3.0",
    "inert": "~4.1.0",
    "joi": "~10.2.2",
    "moment": "^2.29.3",
    "path": "^0.12.7",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "redux": "^4.2.0",
    "vision": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "copyfiles": "^2.1.1",
    "css-loader": "^3.1.0",
    "eslint": "^6.2.1",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^6.2.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "style-loader": "^0.23.1",
    "url-loader": "^4.1.1",
    "webpack": "^4.39.1",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  }
}
